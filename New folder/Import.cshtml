<div class="row">
    <div class="col-md-12 form-group DocumentReg20s" id="excelDiv">
        <div>
            <div class="text-danger">
            </div>
            <div class="row">
                <div class="form-group">
                    <div class="col-md-12">
                        @this.Html.Partial("_FileUpload", new FileUploadViewModel()
                   {
                       FileType = Enums.UploadFileType.File.ToString(),
                       ImageValue = string.Empty,
                       FileTypeExtension = new System.Collections.ObjectModel.Collection<string>() { Enums.FileTypeExtension.Excelx.ToString() }
                   })
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-md-12 text-center">
                        <input type="button" value="Import" class="btn btn-success btnsave upload-excel " data-import="excel" />
                    </div>
                    <div class="downloadTemplt">
                        <div class="Download-Links-wrap">
                            <div class="Download-Links">
                                <div class="col-md-12 col-sm-12 col-xs-12 text-right">
                                    <label class="download-temp download-excel">Download Template</label>
                                    ( <a href="javascript:;" class="download-excel-single singleSheet" title="Download single Sheet Template">Single Sheet </a>
                                    <span class="download-devider">Or</span>
                                    <a href="javascript:;" class="download-excel-single doubleSheet" title="Download Double Sheet Template">Double Sheet </a>)
                                </div>
                                <div class="col-md-12 col-sm-12 col-xs-12 text-right">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-md-12">
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>